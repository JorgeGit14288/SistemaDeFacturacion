<!DOCTYPE html>
<html>
<head>
    <title>Facturacion  | @ViewBag.Title </title>
    <link rel="shortcut icon" src="~/Content/Images/icono.ico">

    <!--  INICIO INCLUSION DE LIBRERIAS CSS DE LA PLANTILLA-->
    @RenderPage("~/Views/Shared/Tema1/Head.cshtml")
    <!--  FIN INCLUSION DE LIBRERIAS CSS DE LA PLANTILLA-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <!--  INICIO header menu superior-->
        @RenderPage("~/Views/Shared/Tema1/Header.cshtml")
        <!--  FIN header menu superior-->
        <!--   Inicio Menu Izquierdo-->
        @RenderPage("~/Views/Shared/Tema1/LeftMenu.cshtml")
        <!--   Fin Menu Izquierdo-->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    @ViewBag.TituloControlador
                    <small>@ViewBag.InfoPagina</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="@Url.Action("Index","Home")"><i class="fa fa-dashboard"></i>INICIO </a></li>
                    <li><a href="#">@ViewBag.TituloControlador</a></li>
                    <li class="active">@ViewBag.Title</li>
                </ol>
            </section>
            <!-- Main content -->
            <section class="content">
                <div class="row">
                    <div class="col-md-12">
                        @Html.Partial("_MsjError")
                        @RenderBody()
                    </div>
                </div>                               
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0
            </div>
            <strong>Copyright &copy; 2016-2017 <a href="#">SISTEMA DE PUNTO DE VENTA</a>.</strong> All rights
            reserved.
        </footer>
        <!-- Control Sidebar -->
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"> </div>
    </div>
    <!--Scripts del pie de pagina-->
    @RenderPage("~/Views/Shared/Tema1/FooterScripts.cshtml")

    @RenderSection("scripts", required: false)
    <!--FIN Scripts del pie de pagina-->
</body>
</html>
